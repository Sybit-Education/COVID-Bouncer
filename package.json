    "dotenv": "^8.2.0",
    "firebase": "^7.23.0",
    "register-service-worker": "^1.7.1",
    "vue": "^2.6.12",
    "vue-i18n": "^8.17.3",
    "vue-notification": "^1.3.20",
    "vue-radial-progress": "^0.3.2",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-eslint": "^4.1.0",
